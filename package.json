{
  "name": "qr-bot",
  "version": "1.0.0",
  "description": "Discord bot to generate a QR code",
  "main": "./build/index.js",
  "repository": "git@github.com:avitretiak/qr-bot.git",
  "author": "Avi Tretiak",
  "scripts": {
    "build": "tsc -p ./src/tsconfig.json",
    "start": "node -r dotenv/config ./build/index.js",
